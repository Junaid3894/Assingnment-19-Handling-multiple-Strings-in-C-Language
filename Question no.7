/*7. From the list of IP addresses, check whether all ip addresses are valid.*/

#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main()
{
char ip[]={"187.368.158.128"};
char *a;
a=strtok(ip,".");

while(a!=NULL){
    int i=atoi(a);
    if(i>=0 && i<=225){
        printf("%d ",i);
        a=strtok(NULL,".");
    }
}
printf("\nYes you have entered a valid ip address");

return 0;
}
